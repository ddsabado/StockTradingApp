<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvSymbol"
        android:layout_width="296dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/textView5"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-medium"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textSize="18sp"
        tools:text="sample" />

    <TextView
        android:id="@+id/tvCurrency"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="69dp"
        android:layout_toEndOf="@+id/tvAmount"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-medium"
        android:textAlignment="textStart"
        android:textSize="18sp"
        tools:text="sample" />

    <TextView
        android:id="@+id/tvAmount"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="69dp"
        android:layout_toEndOf="@+id/textView7"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-medium"
        android:text="sample"
        android:textAlignment="textStart"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvStockName"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="33dp"
        android:layout_toEndOf="@+id/textView6"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-medium"
        android:text="sample"
        android:textAlignment="viewStart"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="2dp"
        android:background="#6075D6"
        android:fontFamily="sans-serif-black"
        android:text="Stock Symbol:"
        android:textAlignment="textEnd"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="15dp"
        android:background="#6075D6"
        android:fontFamily="sans-serif-black"
        android:text="Stock Name:"
        android:textAlignment="textEnd"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="15dp"
        android:background="#6075D6"
        android:fontFamily="sans-serif-black"
        android:text="Stock Price:"
        android:textAlignment="textEnd"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="15dp"
        android:background="#6075D6"
        android:fontFamily="sans-serif-black"
        android:text="Percent Change:"
        android:textAlignment="textEnd"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/tvPercentChangeWL"
        android:layout_width="292dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView8"
        android:layout_marginStart="10dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/textView8"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-medium"
        android:text="sample"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tvAsOf"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="144dp"
        android:background="#86A5A2A2"
        android:text="sample"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="10sp" />

</RelativeLayout>